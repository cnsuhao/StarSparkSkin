<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009" xmlns:s="library://ns.adobe.com/flex/spark" xmlns:mx="library://ns.adobe.com/flex/mx"
		mouseEnabled="false" mouseChildren="false" minWidth="20" minHeight="20">

	<!--- drop shadow can't be hittable so it stays sibling of other graphics @private-->
	<s:RectangularDropShadow id="dropShadow" blurX="3" blurY="3" alpha="0.1" distance="1" angle="90" color="0x000000" left="2" top="0" right="2"
							 bottom="0" />

	<s:Rect id="fill" left="0" right="0" top="0" bottom="0" radiusX="4">
		<s:stroke>
			<s:SolidColorStroke color="0x9e9e9e" />
		</s:stroke>
		<s:fill>
			<s:SolidColor color="0xfffffe" />
		</s:fill>
	</s:Rect>

	<s:Rect id="highlight" left="1" right="1" top="1" bottom="1" radiusX="4">
		<s:stroke>
			<s:SolidColorStroke color="0xffffff" />
		</s:stroke>
	</s:Rect>

</s:Skin>
