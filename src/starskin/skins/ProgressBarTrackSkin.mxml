<!--- The Spark skin class for the MX ProgressBar component's track.

@see mx.controls.ProgressBar

@langversion 3.0
@playerversion Flash 10
@playerversion AIR 1.5
@productversion Flex 4
-->
<s:SparkSkin xmlns:fx="http://ns.adobe.com/mxml/2009" xmlns:s="library://ns.adobe.com/flex/spark" minHeight="8">

	<fx:Script>
		/**
		 * @private
		 */
		override protected function initializationComplete():void
		{
			useChromeColor = true;
			super.initializationComplete();
		}
	</fx:Script>

	<!-- layer 3: fill -->
	<s:Rect left="0" right="0" top="0" bottom="0" radiusX="4">
		<s:fill>
			<s:SolidColor color="0xEEF1F3" />
		</s:fill>
		<s:stroke>
			<s:SolidColorStroke color="0xD4DEE5" />
		</s:stroke>
	</s:Rect>


</s:SparkSkin>
