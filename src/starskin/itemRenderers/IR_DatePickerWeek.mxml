<?xml version="1.0" encoding="utf-8"?>
<s:ItemRenderer xmlns:fx="http://ns.adobe.com/mxml/2009" xmlns:s="library://ns.adobe.com/flex/spark" xmlns:mx="library://ns.adobe.com/flex/mx" enabled="false" disabledAlpha="1">

	<s:Rect width="100%" height="100%">
		<s:fill>
			<s:SolidColor color="#CDDDE3" />
		</s:fill>
		<s:stroke>
			<s:SolidColorStroke color="#ffffff" weight="1" alpha="0.2" />
		</s:stroke>
	</s:Rect>

	<s:Label text="{data}" color="#5BB4C1" horizontalCenter="0" verticalCenter="1" />

</s:ItemRenderer>
